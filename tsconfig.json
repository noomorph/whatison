{
    "compileOnSave": true,
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es3",
        "lib": [
            "ES5",
            "ES2015.Core",
            "ES2015.Promise",
            "ES2015.Iterable"
        ],
        "noImplicitAny": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "rootDir": "src",
        "outDir": "dist"
    }
}
